// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Userc {
  id Int @id @default(autoincrement())
  username String @unique
  password String
  profile String
}

model Chat {
  id Int @id @default(autoincrement())
  username String
  content String
  subcontent String
}
